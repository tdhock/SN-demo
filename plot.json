"yend": "mean",
"showSelected1": "segments" 
},
"params": {
 "lineend": "butt",
"na.rm": false,
"na.rm": false,
"colour": "#00FF00",
"size":        2,
"showSelected": "segments",
"help_showSelected": "segments" 
},
